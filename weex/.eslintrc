{
  "extends": [
    "plugin:vue/essential",
    "standard"
  ],
  "plugins": [
    "vue"
  ],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true

  },
  "rules": {
    "no-debugger": "off",
    "no-console": "off",
    "indent": "off",
    "no-var": "error"
  },
  "globals": {
    "window": true,
    "Vue": true,
    "weex": true
  }
}
