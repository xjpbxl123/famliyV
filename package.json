{
  "name": "piano-family",
  "description": "钢琴家庭版",
  "version": "1.0.0",
  "author": "Moersing Lin<1261870167@qq.com>",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "dev": "cross-env NODE_ENV=development node ./build/webpack.dev.config.js",
    "build": "npm run lint&&cross-env NODE_ENV=production node ./build/build",
    "build:test": "npm run lint&&cross-env NODE_ENV=buildTest node ./build/build",
    "build:testFull": "npm run lint&&cross-env NODE_ENV=buildTest WEEX_ENV=production node ./build/build --fuller",
    "build:weex": "npm run lint&&cross-env NODE_ENV=production node ./build/build --weex",
    "build:full": "npm run lint&&cross-env NODE_ENV=production node ./build/build --fuller",
    "lint": "eslint  --fix --ext js,vue  src build --ignore-pattern !.eslintrc.js --rule no-debugger:error"
  },
  "lint-staged": {
    "*.{json,scss,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.23.0",
    "es6-promise": "^4.1.1",
    "find-sdk": "git+ssh://git@192.168.200.211:FindH5Lib/FindSDK.git#develop",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.0",
    "qrcode": "^1.2.0",
    "vue": "^2.2.1",
    "vue-find": "git+ssh://git@192.168.200.211:FindH5Lib/FindVue.git",
    "vue-find-hybrid": "git+ssh://git@192.168.200.211:zhuzhe/vue-find-hybrid.git#develop",
    "vue-resource": "^1.3.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "cssnano": "^3.10.0",
    "eslint": "^4.10.0",
    "eslint-config-standard": "10.2.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.6",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.0.0",
    "lint-staged": "^6.1.0",
    "node-sass": "^4.5.0",
    "object-assign": "^4.1.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.0.3",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.8.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^0.5.8",
    "vue-loader": "^14.1.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.2.1",
